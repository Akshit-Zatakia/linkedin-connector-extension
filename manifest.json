{
    "manifest_version": 3,
    "name": "LinkedIn Connector",
    "version": "1.0",
    "description": "Automates connection requests on LinkedIn",
    "permissions": [
        "scripting",
        "activeTab"
    ],
    "host_permissions": [
        "https://www.linkedin.com/*"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}